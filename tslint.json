{
  "defaultSeverity": "warning",
  "extends": [],
  "jsRules": {},
  "rules": {
    "no-reference": true,
    "no-var-requires": true,
    "prefer-for-of": true,
    "ban-comma-operator": true,
    "curly": [
      true,
      "ignore-same-line"
    ],
    "forin": true,
    "function-constructor": true,
    "label-position": true,
    "no-arg": true,
    "no-duplicate-switch-case": true,
    "no-implicit-dependencies": [
      true,
      "dev",
      ["@components", "@src", "@selectors", "@stores", "@utils", "@services"]
    ],
    "no-invalid-template-strings": true,
    "no-return-await": true,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-var-keyword": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typeof-compare": true,
    "deprecation": true,
    "eofline": true,
    "indent": [
      true,
      "spaces",
      4
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "max-line-length": [true, 120],
    "no-duplicate-imports": true,
    "arrow-return-shorthand": true,
    "class-name": true,
    "comment-format": [
      false,
      "check-space"
    ],
    "interface-name": [
      true,
      "always-prefix"
    ],
    "no-boolean-literal-compare": true,
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-interfaces",
      "ignore-bound-class-methods"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-rest-spread",
      "check-type",
      "check-typecast",
      "check-type-operator"
    ]
  },
  "rulesDirectory": []
}